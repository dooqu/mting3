<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="MIS_NO_ACTIONBAR" parent="Theme.AppCompat.Light.NoActionBar"></style>
    <declare-styleable name="IntensifyImageView" tools:ignore="ResourceName">
        <attr name="scaleType" format="enum">
            <enum name="fitAuto" value="1" />
            <enum name="fitCenter" value="2" />
        </attr>
        <attr name="animateScaleType" format="boolean" />
        <attr name="maximumScale" format="float" />
        <attr name="minimumScale" format="float" />
        <attr name="scale" format="float" />
    </declare-styleable>

    <declare-styleable name="AnimaImageView" tools:ignore="ResourceName">
        <attr name="auto_play" format="boolean">
        </attr>
    </declare-styleable>
</resources>